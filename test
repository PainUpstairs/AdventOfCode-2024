f = open('day2/data.txt')
input = f.read()
reports = []
for report in input.split("\n"):
    reports.append([int(lvl.strip()) for lvl in report.strip().split(" ")])

# print(reports)

def checkIfLvlValid(report,reverse):
    if reverse:
        report = report[::-1]
    up = []
    oneError = 0
    for lvl in report:
        if lvl in up:
            oneError+=1
        elif len(up) == 0:
            up.append(lvl)
        elif len(up) == 1:
            # if not (1 <= lvl - up[0] <= 3):
            #     oneError+=1
            up.append(lvl)
        elif lvl < up[0] and  lvl < up[1]:
            up.pop(0)
            oneError+=1  

        elif up[1] < up[0] < lvl:
            if 1<= lvl-up[0] <= 3:
                up = [lvl]
            oneError+=1
        elif up[0] < up[1] < lvl:
            if 1<= lvl-up[1] <= 3:
                up = [up[1],lvl]
                if not (1<= up[1]-up[0] <= 3):
                    oneError+=1
            else: 
                oneError+=1
        elif up[1] < lvl < up[0]:
            if 1<= lvl-up[1] <= 3:
                up = [up[1],lvl]
            oneError+=1
        elif up[0] < lvl < up[1]:
            if 1<= lvl-up[0] <= 3:
                up = [up[0],lvl]
            oneError+=1
        if 1 < oneError:
            return False 
    
    return True

print(checkIfLvlValid(reports[8],False))
count = 0
count1 = 0
for report in reports:
    if checkIfLvlValid(report, False) :
        print(False ,report)
        count+=1
        continue
    if checkIfLvlValid(report=report, reverse=True):
        print(True ,report[::-1])
        count+=1
print("count->",count)


